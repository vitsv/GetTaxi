@model WebUI.Models.User.RegisterModel
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title" id="register_modal_lbl">Rejestracja</h4>
</div>
<div class="modal-body">
    @using (Ajax.BeginForm("Register", "User", new AjaxOptions { UpdateTargetId = "af_register_user", HttpMethod = "POST", InsertionMode = InsertionMode.Replace }))
    {
        <div id="af_register_user">
            @Html.Partial("Partial/_registerPartial", Model)
        </div>
    }
</div>
